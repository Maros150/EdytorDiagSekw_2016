package edytor.diagram;

import org.eclipse.graphiti.ui.platform.AbstractImageProvider;

public class EditorImageProvider   extends AbstractImageProvider {
	   // The prefix for all identifiers of this image provider
	   protected static final String PREFIX = "edytor.diagram.";
	   // The image identifier for an Data.
	   public static final String IMG_CLASS= PREFIX + "data";
	   public static final String IMG_MINICLASS= PREFIX + "data";
	   
	   
	@Override
	protected void addAvailableImages() {
		// register the path for each image identifier
	       addImageFilePath(IMG_CLASS, "icons/class.bmp");
	}

}
